				MX6QDL_PAD_GPIO_2__GPIO1_IO02     0x70 //0x80000000  /* panel on       */
				MX6QDL_PAD_GPIO_4__GPIO1_IO04     0x70 //0x80000000  /* backlight on   */
				
				MX6QDL_PAD_EIM_A19__GPIO2_IO19    0x80000000  /* writeprotect spi*/
				
				MX6QDL_PAD_GPIO_3__GPIO1_IO03     0x30b1      /* arduino pinout  */
